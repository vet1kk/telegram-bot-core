{
  "name": "vetikk/telegram-bot-core",
  "description": "Lightweight Telegram Bot PHP Core Library",
  "keywords": [
    "php",
    "library",
    "telegram",
    "bot",
    "webhook",
    "long-polling",
    "core"
  ],
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://github.com/vetikk/telegram-bot-core",
  "support": {
    "issues": "https://github.com/vetikk/telegram-bot-core/issues",
    "source": "https://github.com/vetikk/telegram-bot-core"
  },
  "authors": [
    {
      "name": "Lyskov Vitalii",
      "email": "vet1kk@gmail.com"
    }
  ],
  "type": "library",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "test": "phpunit --colors=always --configuration phpunit.xml",
    "cs": "phpcs",
    "cbf": "phpcbf",
    "psalm": "psalm"
  },
  "require": {
    "php": "~8.3",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.8",
    "psr/container": "^2.0",
    "psr/log": "^3.0",
    "monolog/monolog": "^3.0",
    "symfony/dotenv": "^7.1"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "composer/composer": "^2.8",
    "jetbrains/phpstorm-attributes": "^1.0",
    "phpcompatibility/php-compatibility": "^9.3",
    "phpunit/phpunit": "^9.6",
    "vimeo/psalm": "^5.26"
  },
  "autoload": {
    "psr-4": {
      "Bot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BotTest\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}